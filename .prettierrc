{
    "arrowParens": "avoid",
    "printWidth": 144,
    "quoteProps": "consistent",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "es5",
    "useTabs": false,
    "plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
    "htmlWhitespaceSensitivity": "ignore",
    "overrides": [{ "files": "*.astro", "options": { "parser": "astro" } }]
}
